{
  "name": "wavtrack-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "web",
    "mobile",
    "shared"
  ],
  "scripts": {
    "web": "cd web && npm run dev",
    "mobile": "cd mobile && npm start",
    "dev": "concurrently \"npm run web\" \"npm run mobile\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 